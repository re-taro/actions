{
	"name": "tubo-cache",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@10.18.2",
	"description": "Turborepo cache use GitHub Actions cache",
	"author": {
		"name": "Rintaro Itokawa",
		"email": "me@re-taro.dev",
		"url": "https://re-taro.dev"
	},
	"license": "MIT",
	"main": "lib/index.cjs",
	"scripts": {
		"build": "esbuild src/main.ts --bundle --minify --platform=node --target=es2021 --outfile=lib/index.cjs",
		"lint": "run-s -c lint:*",
		"lint:eslint": "eslint .",
		"lint:ts": "tsc -p . --noEmit",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm lint:eslint --fix",
		"test": "vitest --run"
	},
	"dependencies": {
		"@actions/core": "1.11.1",
		"axios": "1.12.2",
		"fastify": "5.6.1",
		"lazy-strict-env": "0.3.1",
		"wait-on": "8.0.5",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@re-taro/eslint-config": "5.3.0",
		"@tsconfig/strictest": "2.0.6",
		"@types/node": "22.18.9",
		"@types/wait-on": "5.3.4",
		"esbuild": "0.25.10",
		"eslint": "9.37.0",
		"eslint-plugin-format": "0.1.2",
		"npm-run-all2": "7.0.2",
		"typescript": "5.9.3",
		"vitest": "3.2.4"
	}
}
