{
  "name": "tubo-cache",
  "description": "Turborepo cache use GitHub Actions cache",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@8.9.0",
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "main": "lib/index.cjs",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --minify --platform=node --target=es2021 --outfile=lib/index.cjs",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint . --ext .ts,.cjs,json",
    "lint:prettier": "prettier --check **/*.{ts,cjs,json}",
    "lint:ts": "tsc -p . --noEmit",
    "fmt": "run-s -c fmt:*",
    "fmt:eslint": "pnpm lint:eslint --fix",
    "fmt:prettier": "prettier --write **/*.{ts,cjs,json}",
    "test": "vitest --run"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "axios": "1.5.1",
    "fastify": "4.24.1",
    "lazy-strict-env": "0.3.1",
    "wait-on": "7.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@re-taro/eslint-config": "1.10.3",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20.8.5",
    "@types/wait-on": "5.3.2",
    "esbuild": "0.19.4",
    "eslint": "8.51.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  }
}
