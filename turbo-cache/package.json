{
	"name": "tubo-cache",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@8.15.7",
	"description": "Turborepo cache use GitHub Actions cache",
	"author": {
		"name": "Rintaro Itokawa",
		"email": "me@re-taro.dev",
		"url": "https://re-taro.dev"
	},
	"license": "MIT",
	"main": "lib/index.cjs",
	"scripts": {
		"build": "esbuild src/main.ts --bundle --minify --platform=node --target=es2021 --outfile=lib/index.cjs",
		"lint": "run-s -c lint:*",
		"lint:eslint": "eslint .",
		"lint:ts": "tsc -p . --noEmit",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm lint:eslint --fix",
		"test": "vitest --run"
	},
	"dependencies": {
		"@actions/core": "1.10.1",
		"axios": "1.6.8",
		"fastify": "4.26.2",
		"lazy-strict-env": "0.3.1",
		"wait-on": "7.2.0",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@re-taro/eslint-config": "4.1.0",
		"@tsconfig/strictest": "2.0.5",
		"@types/node": "20.12.7",
		"@types/wait-on": "5.3.4",
		"esbuild": "0.20.2",
		"eslint": "9.0.0",
		"eslint-plugin-format": "0.1.0",
		"npm-run-all": "4.1.5",
		"typescript": "5.4.5",
		"vitest": "1.5.0"
	}
}
