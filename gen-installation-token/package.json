	"devDependencies": {
		"@re-taro/eslint-config": "5.3.0",
		"@tsconfig/strictest": "2.0.6",
		"@types/is-base64": "1.1.3",
		"@types/node": "22.18.9",
		"esbuild": "0.25.4",
		"eslint": "9.37.0",
		"eslint-plugin-format": "0.1.2",
		"npm-run-all2": "7.0.2",
		"typescript": "5.9.3"
