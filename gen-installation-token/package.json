		"is-base64": "1.1.0"
	},
	"devDependencies": {
		"@re-taro/eslint-config": "5.2.0",
		"@tsconfig/strictest": "2.0.5",
		"@types/is-base64": "1.1.3",
		"@types/node": "20.16.10",
		"esbuild": "0.24.0",
		"eslint": "9.11.1",
		"eslint-plugin-format": "0.1.2",
		"npm-run-all": "4.1.5",
		"typescript": "5.6.2"
	}
}
